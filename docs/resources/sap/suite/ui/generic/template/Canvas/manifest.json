		"services": {
			"ShellUIService": {
				"factoryName": "sap.ushell.ui5service.ShellUIService"
			}
		},
		"dependencies": {
			"minUI5Version": "1.30.1",
			"libs": {
				"sap.ui.core": {
					"minVersion": "1.30.1"
				},
				"sap.m": {
					"minVersion": "1.30.1"
				},
				"sap.f": {
					"minVersion": "1.41.0"
				},
				"sap.ui.comp": {
					"minVersion": "1.30.1"
